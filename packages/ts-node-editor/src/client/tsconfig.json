{
  "extends": "./../../tsconfig.json",
  "include": [
    "client.ts",
    "element/button/button.ts",
    "element/comment/comment.ts",
    "element/header/header.ts",
    "element/icon/icon.ts",
    "element/menu/menu.ts",
    "element/nodeContainer/nodeContainer.ts",
    "element/nodeHandle/nodeHandle.ts",
    "element/nodeConnection/nodeConnection.ts",
    "element/seperator/seperator.ts",
    "element/tooltip/tooltip.ts",
    "utils/tooltip.ts",
  ],
  "compilerOptions": {
    "outDir": "./../../dist/client",
    "baseUrl": ".",
    "composite": true,
    "module": "NodeNext",
    "target": "ES6",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "node",
      "jest",
      "@pictogrammers/element-jest"
    ],
    "paths": {
      "@pictogrammers/element": [
        "./node_modules/@pictogrammers/element"
      ]
    }
  }
}